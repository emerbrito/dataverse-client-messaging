<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

	<!-- nuget package metadata starts -->
	<Version>0.0.1</Version>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<Authors>Emerson Brito</Authors>
	<Owners>emerbrito</Owners>
	<Description>A message dispatcher implementation for Azure Functions with Service Bus triggers. Abstracts message handling from the Azure Function trigger and allow a single message to be dispatched to multiple handlers.</Description>
	<Copyright>Â© Emerson Brito. All rights reserved.</Copyright>
	<PackageProjectUrl>https://github.com/emerbrito/dataverse-client-messaging</PackageProjectUrl>
	<RepositoryUrl>https://github.com/emerbrito/dataverse-client-messaging</RepositoryUrl>
	<PackageTags>dataverse;dynamics;commondataservice;cds;powerapps,powerplatform;serviceclient;sdk;service bus</PackageTags>
	<PackageReleaseNotes>
		0.0.1:
		Initial release.
	</PackageReleaseNotes>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	<PackageReadmeFile></PackageReadmeFile>
	<!-- nuget package metadata ends -->

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="6.0.0" />
    <PackageReference Include="Microsoft.PowerPlatform.Dataverse.Client" Version="1.0.26" />
  </ItemGroup>

</Project>
